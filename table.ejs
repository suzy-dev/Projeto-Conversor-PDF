<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
</head>
<body>

    <table>
        <thead>  
            
        <% header.forEach(col => { %>
            <th> <%= col %> </th>
        <% }) %>

        </thead>
        <tbody>
            <% row.forEach(rows => { %>
                <tr>
                    <% rows.forEach(cell => { %>
                        <td> <%= cell %> </td>
                    <% }) %>
                </tr>
            <% }) %>
        </tbody>
        
        

        
    </table>

</body>
</html>